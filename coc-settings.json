{
  "languageserver": {
    "typescript": {
      "command": "typescript-language-server",
      "args": ["--stdio"],
      "filetypes": ["typescript", "typescriptreact"],
      "initializationOptions": {
        "tsconfigPath": "./tsconfig.json"
      }
    }
  },
  "tree-sitter": {
    "typescript": {
      "enabled": true
    },
    "javascript": {
      "enabled": true
    },
    "css": {
      "enabled": true
    },
    "jsx": {
      "enabled": true
    }
  }
}
